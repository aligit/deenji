{
  "name": "@deenji/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "supabase:start": "cd $HOME/work-public/deenji-supabase && bunx supabase start -x edge-runtime,vector,logflare",
    "supabase:stop": "cd $HOME/work-public/deenji-supabase && bunx supabase stop  --all",
    "supabase:status": "cd $HOME/work-public/deenji-supabase && bunx supabase status",
    "es:start": "docker compose -f elasticsearch-compose.yml up -d",
    "es:stop": "docker compose -f elasticsearch-compose.yml down",
    "es:restart": "bun run es:stop && bun run es:start",
    "es:status": "docker ps --filter \"name=tuqdar_elasticsearch\"",
    "dev:services": "bun run supabase:start && bun run es:start",
    "stop:services": "bun run es:stop && bun run supabase:stop",
    "dev:start": "./scripts/start-dev-environment.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "private": true,
